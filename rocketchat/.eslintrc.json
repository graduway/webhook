{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "overrides": [{
    "files": [
      "*.js"
    ],
    "rules": {
      "camelcase": "off"
    }
  }],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
